import numpy as np


test_vector = np.array([12,3,1,3])


# 2.2

def norm_manual(x):
    return np.power(np.power(x,2).sum(), 0.5)


test_vector = np.array([12,3,1,3])

print(norm_manual(test_vector) == np.linalg.norm(test_vector))



# 2.3

def zero_vector(x):
    return x*0.0

print(zero_vector(test_vector))




# 2.4

def vector_scaler(x, magnitude):
    
    scale_factor = magnitude/np.linalg.norm(x)

    return x*scale_factor

print(vector_scaler(test_vector, 1))




